@model List<Employee>

<h2>Employee Table</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Total Time Worked</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr class="@(employee.TotalTimeWorked < 100 ? "underNorm" : "")">
                <td>@employee.EmployeeName</td>
                <td>@employee.TotalTimeWorked</td>
            </tr>
        }
    </tbody>
</table>
